<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="PlayEJB">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>java/oracle</jta-data-source>
		<class>net.mrsistemas.play.model.entities.GenCiudad</class>
		<class>net.mrsistemas.play.model.entities.GenGaleria</class>
		<class>net.mrsistemas.play.model.entities.GenPais</class>
		<class>net.mrsistemas.play.model.entities.GenPerfil</class>
		<class>net.mrsistemas.play.model.entities.TourDatosUsuario</class>
		<class>net.mrsistemas.play.model.entities.TourMenu</class>
		<class>net.mrsistemas.play.model.entities.TourParametro</class>
		<class>net.mrsistemas.play.model.entities.TourRole</class>
		<class>net.mrsistemas.play.model.entities.TourRolesUsuario</class>
		<class>net.mrsistemas.play.model.entities.TourUsuario</class>
		<class>net.mrsistemas.play.model.entities.GenSession</class>
		<class>net.mrsistemas.play.model.entities.ElectronicCategory</class>
		<class>net.mrsistemas.play.model.entities.ElectronicDevice</class>
		<properties>
			<property name="eclipselink.target-server" value="WebLogic" />
			<property name="eclipselink.logging.level" value="FINEST" />
			<property name="eclipselink.weaving.lazy" value="true" />
			<property name="eclipselink.weaving.eager" value="true" />
		</properties>
	</persistence-unit>
</persistence>
